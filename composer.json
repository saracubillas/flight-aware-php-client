{
    "name": "saracubillas/flight-aware-php-client",
    "description": "very simple PHP client for the FlightAware API",
    "keywords": [
        "flightaware",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.5 || ^2.0"
    },
    "autoload": {
        "psr-4": { "FlightAware\\PHPClient\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "FlightAware\\PHPClient\\" : "test/" }
    }
}
